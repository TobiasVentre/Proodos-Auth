{
  "name": "@proodos/tests",
  "version": "1.0.0",
  "private": true,
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.0.0"
  },
  "scripts": {
    "clean": "node -e \"require('fs').rmSync('dist',{recursive:true,force:true});require('fs').rmSync('coverage',{recursive:true,force:true});\"",
    "build": "tsc -p .",
    "test": "node -e \"require('jest').run(['--config','jest.config.cjs'])\"",
    "test:coverage": "node -e \"require('jest').run(['--config','jest.config.cjs','--coverage'])\"",
    "test:watch": "node -e \"require('jest').run(['--config','jest.config.cjs','--watch'])\""
  }
}
